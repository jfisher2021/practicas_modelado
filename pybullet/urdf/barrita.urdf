<robot name="barrita">

    <material name="red">
        <color rgba="1.0 0.1 0.20 1.0" />
    </material>

    <link name="cilindro">
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="300.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
        <visual name="">
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0" />
            <geometry>
                <cylinder radius="0.2" length="1.0" />
            </geometry>
            <material name="red" />
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0" />
            <geometry>
                <cylinder radius="0.2" length="1.0" />
            </geometry>
        </collision>
    </link>

    <link
        name="barra">
        <inertial>
            <origin xyz="0 -0.5 -0.2" rpy="0 0  0" />
            <mass value="5.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
        <visual name="">
            <origin xyz="0 -0.5 -0.2" rpy="0 0 0" />
            <geometry>
                <box size="0.1 2.0 0.1" />
            </geometry>
            <material name="red" />
        </visual>

        <collision>
            <origin xyz="0 -0.5 -0.2" rpy="0 0 0" />
            <geometry>
                <box size="0.1 2.0 0.1" />
            </geometry>
        </collision>
    </link>
    <joint
        name="not_to_pass" type="continuous">
        <parent link="cilindro" />
        <child link="barra" />
        <origin xyz="0.0 0.0 0.0" rpy="0 0.0 0" />
        <dynamics damping="0.0" friction="0.00001" />
        <axis xyz="0.0 0.0 1.0" />

    </joint>
</robot>